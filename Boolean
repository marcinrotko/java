import java.util.Scanner;

public class Boolean {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		
		//== porównanie równa się
		//!= różny
		//>= <= > < 
		//&& = AND = i
		// || OR = LUB
		boolean condition= a>=5 && a<15;
		
		System.out.println(condition);

	}

}
