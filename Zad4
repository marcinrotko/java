import java.util.Scanner;

public class Zad4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		if (a>0){
			System.out.println("Liczna dodatnia");
		}
			else if(a==0){
				System.out.println("Liczba rowna zero");
			}else{
			System.out.println("Liczba ujemna");
				
			}
		}
	}
